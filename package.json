{
  "name": "cinema_fe",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "generate:icons": "node scripts/generateIconsTypes.js",
    "check-node-version": "node -e \"const v=process.versions.node.split('.');if(+v[0]<24||(v[0]==24&&+v[1]<9)||+v[0]>=25)throw new Error('Node version must be >= 24.0.0 and < 25.0.0')\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "dev:internal": "node scripts/updateBuildWorkflow.js && npm run check-node-version && npm run generate:icons && npm i && node ./server.mjs",
    "dev": "cross-env REACT_COMPILER=1 npm run dev:internal",
    "build": "cross-env REACT_COMPILER=1 node genBuild.js",
    "build-core-used-internally-do-not-edit": "npm run check-node-version && react-router build",
    "start": "cross-env NODE_ENV=production ./node_modules/dotenv-cli/cli.js -e .env node server.mjs",
    "test": "vitest"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "1.2.8",
    "@react-router/node": "^7.9.2",
    "@react-router/remix-config-routes-adapter": "0.0.0-nightly-bf7ecb711-20240911",
    "@react-router/serve": "^7.9.2",
    "@tailwindcss/typography": "0.5.19",
    "@vitejs/plugin-react": "5.1.1",
    "babel-plugin-react-compiler": "1.0.0",
    "class-variance-authority": "0.7.1",
    "color": "5.0.3",
    "colors": "1.4.0",
    "dotenv": "17.2.3",
    "isbot": "^5.1.31",
    "js-yaml": "^4.1.1",
    "lucide": "0.555.0",
    "lucide-react": "0.563.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.2",
    "remix-flat-routes": "^0.8.5",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7",
    "vite-plugin-babel": "1.3.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "10.4.23",
    "baseline-browser-mapping": "2.9.11",
    "cross-env": "10.1.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-curly": "0.4.1",
    "react-router-devtools": "6.0.0",
    "tailwindcss": "3.4.19",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-svgr": "4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
